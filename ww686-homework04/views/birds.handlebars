<h3>Sightings</h3>
<ul id = "sightings">
  {{#each birds}}
  <li><h3><span id = "quantity">{{this.quantity}}</span> x {{this.name}}</h3></li>
  {{/each}}
</ul>

<form method="POST" action="">
  <input type="text" name="birdName">
  <br>
  <input type="submit" value = "Add Another Bird">
</form>

<p><a href = "settings">(My Settings)</a></p>
